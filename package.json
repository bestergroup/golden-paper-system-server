{
  "name": "server",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "typeorm": "typeorm-ts-node-commonjs",
    "migrate": "npm run drop-tables && npx knex migrate:latest --knexfile ./database/knexfile.ts",
    "migrate-file": "npx knex migrate:make tables --migrations-directory ./migrations --knexfile ./database/knexfile.ts",
    "delete-tables": "npx knex seed:run --knexfile ./database/knexfile.ts --specific=delete.ts",
    "drop-tables": "npx knex seed:run --knexfile ./database/knexfile.ts --specific=drop.ts",
    "update-types": "npx ts-node ./database/config.ts",
    "seed:role": "npx knex seed:run --knexfile ./database/knexfile.ts --specific=role.ts",
    "seed:part": "npx knex seed:run --knexfile ./database/knexfile.ts --specific=part.ts",
    "seed:city": "npx knex seed:run --knexfile ./database/knexfile.ts --specific=city.ts",
    "seed:role-part-default": "npx knex seed:run --knexfile ./database/knexfile.ts --specific=role-part-default.ts",
    "seed:user": "npx knex seed:run --knexfile ./database/knexfile.ts --specific=user.ts",
    "seed:user-part": "npx knex seed:run --knexfile ./database/knexfile.ts --specific=user-part.ts",
    "seed:expense": "npx knex seed:run --knexfile ./database/knexfile.ts --specific=expense.ts",
    "seed:mandub": "npx knex seed:run --knexfile ./database/knexfile.ts --specific=mandub.ts",
    "seed:customer": "npx knex seed:run --knexfile ./database/knexfile.ts --specific=customer.ts",
    "seed:customer": "npx knex seed:run --knexfile ./database/knexfile.ts --specific=customer.ts",
    "seed:case": "npx knex seed:run --knexfile ./database/knexfile.ts --specific=case.ts",
    "seed:item_type": "npx knex seed:run --knexfile ./database/knexfile.ts --specific=item_type.ts",
    "seed:case_history": "npx knex seed:run --knexfile ./database/knexfile.ts --specific=case_history.ts",
    "seed:employee": "npx knex seed:run --knexfile ./database/knexfile.ts --specific=employee.ts",
    "seed:expense_type": "npx knex seed:run --knexfile ./database/knexfile.ts --specific=expense_type.ts",
    "seed:item": "npx knex seed:run --knexfile ./database/knexfile.ts --specific=item.ts",
    "seed:config": "npx knex seed:run --knexfile ./database/knexfile.ts --specific=config.ts",
    "seed:item_history": "npx knex seed:run --knexfile ./database/knexfile.ts --specific=item_history.ts",
    "seed:all": "npm run delete-tables && npm run seed:city && npm run seed:role && npm run seed:part && npm run seed:user && npm run seed:user-part && npm run seed:expense && npm run seed:mandub && npm run seed:customer && npm run seed:customer  && npm run seed:case && npm run seed:case_history && npm run seed:employee && npm run seed:role-part-default && npm run seed:item && npm run seed:config && npm run seed:item_history",
    "pg-types": "npx knex-types generate --client=pg --connection=postgresql://postgres:postgres@localhost:5432/management_system --out=./database/types.ts",
    "pg-types-2": "npx knex-types --config ./database/knexfile.ts --output ./database/types.ts"
  },
  "dependencies": {
    "@casl/ability": "^6.7.1",
    "@faker-js/faker": "^8.4.1",
    "@nestjs/common": "^10.0.0",
    "@nestjs/config": "^3.2.3",
    "@nestjs/core": "^10.0.0",
    "@nestjs/devtools-integration": "^0.1.6",
    "@nestjs/jwt": "^10.2.0",
    "@nestjs/mapped-types": "*",
    "@nestjs/passport": "^10.0.3",
    "@nestjs/platform-express": "^10.0.0",
    "@nestjs/swagger": "^7.4.0",
    "@nestjs/throttler": "^6.1.0",
    "@nestjs/typeorm": "^10.0.2",
    "@types/bcryptjs": "^2.4.6",
    "@types/cors": "^2.8.17",
    "@types/pdfkit": "^0.13.5",
    "@types/pg": "^8.11.8",
    "bcryptjs": "^2.4.3",
    "canvas": "^2.11.2",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "csurf": "^1.11.0",
    "dotenv": "^16.4.5",
    "express-session": "^1.18.0",
    "helmet": "^7.1.0",
    "joi": "^17.13.3",
    "jsbarcode": "^3.11.6",
    "knex": "^3.1.0",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "pdfkit": "^0.15.0",
    "pg": "^8.12.0",
    "puppeteer": "^23.4.1",
    "puppeteer-core": "^23.4.1",
    "reflect-metadata": "^0.2.0",
    "rxjs": "^7.8.1",
    "typeorm": "^0.3.20"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.0.0",
    "@nestjs/schematics": "^10.0.0",
    "@nestjs/testing": "^10.0.0",
    "@types/cookie-parser": "^1.4.7",
    "@types/express": "^4.17.17",
    "@types/express-session": "^1.18.0",
    "@types/jest": "^29.5.2",
    "@types/node": "^20.3.1",
    "@types/supertest": "^6.0.0",
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^7.0.0",
    "eslint": "^8.42.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "jest": "^29.5.0",
    "knex-types": "^0.5.0",
    "prettier": "^3.0.0",
    "source-map-support": "^0.5.21",
    "supertest": "^7.0.0",
    "ts-jest": "^29.1.0",
    "ts-loader": "^9.4.3",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.1.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
